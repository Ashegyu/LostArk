<Window x:Class="Http.View.FindAccWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Http.View"
        mc:Ignorable="d"
        Title="FindAccWindow" Height="450" Width="800">
    <Grid>
        <ListBox Margin="10"
            ItemsSource="{Binding AccCollectionView}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Grid Margin="5">
                        <Grid.RowDefinitions>
                            
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <TextBlock Margin="2" Text="{Binding TotalPrice}"></TextBlock>
                        <Grid Margin="2" Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Margin="2"
                                Grid.ColumnSpan="2"
                                Text="{Binding NeckAblity.Name}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Text="가격 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Grid.Column="1"
                                Text="{Binding NeckAblity.Price}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Text="품질 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Grid.Column="1"
                                Text="{Binding NeckAblity.Quality}"></TextBlock>
                        </Grid>
                        <Grid
                            Margin="2"
                            Grid.Column="2">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Margin="2"
                                Grid.ColumnSpan="2"
                                Text="{Binding FirstRingAblity.Name}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Text="가격 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Grid.Column="1"
                                Text="{Binding FirstRingAblity.Price}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Text="품질 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Grid.Column="1"
                                Text="{Binding FirstRingAblity.Quality}"></TextBlock>
                        </Grid>
                        <Grid
                            Margin="3"
                            Grid.Column="3">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Margin="2"
                                Grid.ColumnSpan="2"
                                Text="{Binding SecondRingAblity.Name}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Text="가격 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Grid.Column="1"
                                Text="{Binding SecondRingAblity.Price}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Text="품질 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Grid.Column="1"
                                Text="{Binding SecondRingAblity.Quality}"></TextBlock>
                        </Grid>
                        <Grid
                            Margin="4"
                            Grid.Column="4">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Margin="2"
                                Grid.ColumnSpan="2"
                                Text="{Binding FirstEarAblity.Name}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Text="가격 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Grid.Column="1"
                                Text="{Binding FirstEarAblity.Price}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Text="품질 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Grid.Column="1"
                                Text="{Binding FirstEarAblity.Quality}"></TextBlock>
                        </Grid>
                        <Grid
                            Margin="5"
                            Grid.Column="5">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Margin="2"
                                Grid.ColumnSpan="2"
                                Text="{Binding SecondEarAblity.Name}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Text="가격 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="1"
                                Grid.Column="1"
                                Text="{Binding SecondEarAblity.Price}"></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Text="품질 : "></TextBlock>
                            <TextBlock
                                Margin="2"
                                Grid.Row="2"
                                Grid.Column="1"
                                Text="{Binding SecondEarAblity.Quality}"></TextBlock>
                        </Grid>
                    </Grid>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>
